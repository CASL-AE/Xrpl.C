{
    "metadata": [
        {
            "src": [
                {
                    "files": [ "*.csproj" ],
                    "src": "../Xrpl"
                },
                {
                    "files": [ "*.csproj" ],
                    "src": "../Base/Xrpl.AddressCodec"
                },
                {
                    "files": [ "*.csproj" ],
                    "src": "../Base/Xrpl.BinaryCodec"
                },
                {
                    "files": [ "*.csproj" ],
                    "src": "../Base/Xrpl.Keypairs"
                }
            ],
            "dest": "reference",
            "disableGitFeatures": false,
            "disableDefaultFilter": false,
            "filter": "filterConfig.yml"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "reference/**.yml",
                    "reference/index.md"
                ]
            },
            {
                "files": [
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [ "images/**" ]
            }
        ],
        "dest": "../docs",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [ "statictoc", "templates/unity" ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}